set terminal svg background "white"
set key bottom right
set output "Gamma.svg"
set xlabel "x"
set ylabel "ln(y)"
set tics out
set xzeroaxis
set yzeroaxis
set samples 800
set key right top
set title "Fit to exdata"
plot [0:16][0:2.5] \
 "plotdata.data" with lines linetype 1 title "best fit" \
, "plotdata_minus.data" with lines linetype 4 title "best fit minus unc" \
, "plotdata_plus.data" with lines linetype 4 title "best fit plus unc" \
,"actualdata.data" with errorbars title "datapoints" \
