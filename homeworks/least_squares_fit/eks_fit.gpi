set terminal svg
set key bottom right
set output "Gamma.svg"
set xlabel "x"
set ylabel "y"
set tics out
set xzeroaxis
set yzeroaxis
set samples 800
set title "Fit to exdata"
plot [0:16][0:200] \
 "plotdata.data" with lines linetype 1 title "theory" \
,"actualdata.data" using ($1+1):($2) with points pointtype 4 title "experiment" \
#,gamma(x) with lines linetype 2 title "built-in gamma"
