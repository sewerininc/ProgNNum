set terminal svg background "white"
set key bottom right
set output "Gamma.svg"
set xlabel "x"
set ylabel "ln(y)"
set tics out
set xzeroaxis
set yzeroaxis
set samples 800
set title "Fit to exdata"
plot [0:16][0:2.5] \
 "plotdata.data" with lines linetype 1 title "theory" \
, "plotdata_minus.data" with lines linetype 1 title "theory minus unc" \
, "plotdata_plus.data" with lines linetype 1 title "theory plus unc" \
,"actualdata.data" with errorbars title "experiment" \
